{
  "name": "saving-pool-contract",
  "version": "1.0.0",
  "description": "Saving pool contract.",
  "main": "index.js",
  "scripts": {
    "compile": "truffle compile",
    "test": "yarn prepare && truffle test",
    "generate": "truffle compile && typechain --target truffle './build/**/*.json'",
    "prepare": "yarn generate",
    "coverage": "npx solidity-coverage",
    "coverage-setup": "node_modules/.bin/testrpc-sc --gasLimit 0x1fffffffffffff --gasPrice 1 --defaultBalanceEther 1000000000 --allowUnlimitedContractSize true",
    "lint": "npx solium -d contracts/",
    "ganache": "ganache-cli -p 8545 -l 10000000 -e 10000 --allowUnlimitedContractSize"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DeFinerOrg/Savings.git"
  },
  "keywords": [
    "Saving",
    "Pool",
    "Contract"
  ],
  "author": "Mark Li",
  "license": "ISC",
  "devDependencies": {
    "@chainlink/contracts": "0.0.3",
    "@codechecks/client": "^0.1.10",
    "@openzeppelin/test-helpers": "^0.5.5",
    "@openzeppelin/upgrades": "^2.8.0",
    "@typechain/truffle-v5": "^0.9.0",
    "eth-gas-reporter": "^0.2.17",
    "ethlint": "^1.2.5",
    "husky": "^4.2.5",
    "openzeppelin-solidity": "^2.4.0",
    "solc": "0.5.14",
    "solidity-coverage": "^0.6.7",
    "surya": "^0.4.0",
    "truffle": "^5.1.24",
    "truffle-typings": "^1.0.7",
    "ts-generator": "^0.0.8",
    "ts-node": "^7.0.1",
    "typechain": "^1.0.0",
    "typechain-target-truffle": "^1.0.0",
    "typescript": "^3.1.6",
    "web3": "^1.2.8",
    "web3-utils": "1.2.6"
  },
  "dependencies": {
    "@types/shelljs": "^0.8.8",
    "mocha": "^8.1.1",
    "shelljs": "^0.8.4",
    "solidity-parser-antlr": "^0.4.11"
  }
}